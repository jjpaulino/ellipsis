Host *
    LogLevel QUIET
    ServerAliveCountMax 5
    ServerAliveInterval 120
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

Host github.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/personal_ed25519

Host gitlab.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/personal_ed25519

Host think.fleet.i.ng
    HostName ec2-18-223-160-184.us-east-2.compute.amazonaws.com
    IdentitiesOnly yes
    IdentityFile ~/.ssh/personal_rsa
